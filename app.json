{
  "expo": {
    "name": "Giftyy",
    "slug": "giftyy",
    "version": "1.0.0",
    "description": "Making gifting fun and memorable",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "giftyy",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#f75507",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to record a short video message for the gift.",
        "NSMicrophoneUsageDescription": "This app uses the microphone while recording your video message.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.giftyy"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "versionCode": 25,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "package": "com.anonymous.giftyy",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.giftyy.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/giftyy.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "react-native-vision-camera",
      "expo-video",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "67ba6b4a-d0c0-4d22-89f3-8a2d2fc09a36"
      }
    },
    "owner": "rtarbaris-organization"
  }
}
